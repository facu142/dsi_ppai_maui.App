<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="dsi_ppai_maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:dsi_ppai_maui"
    Shell.FlyoutBehavior="Disabled"
    xmlns:views="clr-namespace:dsi_ppai_maui.Views">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:HomeView}"
        Route="MainPage" />

    <ShellContent
        Title="Consultar Encuesta"
        ContentTemplate="{DataTemplate views:ConsultarEncuestaView}"
        Route="ConsutarEncuesta" />

</Shell>
